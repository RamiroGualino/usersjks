			"script": {
				"type": "text/javascript",
				"exec": [
					"// Define el tiempo de espera en milisegundos (ejemplo: 4000 ms = 4 segundos)",
					"const delay = 4000;",
					"",
					"// Obtiene la lista de solicitudes en la colecci√≥n",
					"const requests = pm.collection.requests.all();",
					"",
					"// Itera sobre cada solicitud y agrega un tiempo de espera antes de ejecutarla",
					"for (let i = 0; i < requests.length; i++) {",
					"  setTimeout(() => {",
					"    pm.collection.request(requests[i].id);",
					"  }, delay * i);",
					"}"
				]
			}
		},
